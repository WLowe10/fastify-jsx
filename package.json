{
	"name": "fastify-jsx-router",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"name": "Wes Lowe",
		"email": "wesley.jlowe10@gmail.com"
	},
	"keywords": [
		"fastify",
		"jsx",
		"react"
	],
	"repository": {
		"type": "git",
		"url": ""
	},
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier --write .",
		"publish-package": "pnpm build && pnpm publish",
		"clean": "pnpm clean:dist && pnpm clean:modules",
		"clean:dist": "rm -rf dist",
		"clean:modules": "rm -rf node_modules",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"fastify": "^5.0.0",
		"react": "^18.3.1"
	},
	"dependencies": {
		"react-nil": "^1.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.3.11",
		"prettier": "^3.3.2",
		"react": "^18.3.1",
		"tsup": "^8.1.0",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3"
	}
}
